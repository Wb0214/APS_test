<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape>
            <!--  dashWidth:虛線長度，dashGap:虛線間隔  -->
            <stroke
                android:color="#000000"
                android:width="1dp"
                android:dashWidth="2dp"
                android:dashGap="2dp"/>
        </shape>
    </item>
</layer-list>